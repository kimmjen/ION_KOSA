package ex06;

public class MainEntry {
	public static void main(String[] args) {
		System.out.println("main start!!!");
		
		line(); // 호출
		System.out.println("main end");
	}
	// 1) 매개변수 없고, 리턴타입 없는경우
	public static void line() {
		System.out.println("--------------------------------");
	}
	
	
	

}
