package ex02.array;

public class MainEntry {
	public static void main(String[] args) {
		// 1.
		int[][] a = new int[2][3]; // 행크기, 열크기
		
		a[0][0] = 1; //초기화 작업
		a[0][1] = 2;
		a[0][2] = 3;
		
		a[1][0] = 4;
		a[1][1] = 5;
		a[1][2] = 6;
		
//		a[2][0] = 7;
//		a[2][1] = 8;
//		a[2][2] = 9;
		
		// 2.
		int[][] a2 = {{1,2,3},{4,5,6}};
		int[][] a3 = new int[][] {{1,2,3},{4,5,6}};
		
		// 배열값 출력
		System.out.println("a2[0][2] = " + a2[0][2]);
		
	}
}
