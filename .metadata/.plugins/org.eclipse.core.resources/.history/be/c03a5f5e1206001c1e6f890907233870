package ex03.Switch;

import java.util.Scanner;

/*
 * */
public class SwitchEx01 {

	public static void main(String[] args) {
		System.out.println("학점 : ");
		int score = new Scanner(System.in).nextInt();
		char grade = ' ';

		switch (score) {
		case 100:
			grade = 'A';
			break;

		case 90:
			grade = 'B';
			break;

		case 80:
			grade = 'C';
			break;

		case 70:
			grade = 'D';
			break;
		case 60:
			grade = 'E';
			break;

		default:
			grade = 'F';
			break;
		} // switch end
		System.out.println("\n\n 점수는 : " + score + "학점은 : " + grade);
	}

}
