package home;

public class p_8 {

	public static void main(String[] args) {
		System.out.println("정수 두개를 입력하세요.");
		int a = new Scanner(System.in).nextInt();
		int b = new Scanner(System.in).nextInt();
		try {
			System.out.println(a + " / " + b + " = " + (a / b));
		} catch (Exception e) {
			// TODO: handle exception
			e.printStackTrace();
		}
	}
}