package day03_basic.ex03.IF;

public class MainIF {
    public static void main(String[] args) {
        int score = 88;
        // 평균(학점) 구하는 프로그램 만들기 (if)
        if (score >= 90) {
            System.out.println("A");
        } else if (score >= 80) {
            System.out.println("B");
        } else if (score >= 70) {
            System.out.println("C");
        } else if (score >= 60) {
            System.out.println("D");
        } else {
            System.out.println("F");
        }
    }
}
