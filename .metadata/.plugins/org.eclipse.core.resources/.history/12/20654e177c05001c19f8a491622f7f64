package day03_basic.Quiz;

import java.util.Scanner;

public class Q_2 {
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		System.out.println("x: " + "y: " + "z: ");
		
		int x = sc.nextInt();
		int y = sc.nextInt();
		int z = sc.nextInt();
		
		System.out.println(x);
		System.out.println(y);
		System.out.println(z);
	}
}
